        [api = "coreCount",              mapping = "computerDetails.number_cores"],
        [api = "processorType",          mapping = "computerDetails.processor_type"],
        [api = "processorArchitecture",  mapping = "computerDetails.processor_architecture"],
        [api = "busSpeedMhz",            mapping = "computerDetails.bus_speed_mhz"],
        [api = "cacheSizeKilobytes",     mapping = "computerDetails.cache_size_kb"],
        [api = "macAddress",             mapping = "computerDetails.mac_address"],
        [api = "altMacAddress",          mapping = "computerDetails.alt_mac_address"],
        [api = "totalRamMegabytes",      mapping = "computerDetails.total_ram_mb"],
        [api = "openRamSlots",           mapping = "computerDetails.available_ram_slots"],
        [api = "batteryCapacityPercent", mapping = "computerDetails.battery_capacity"],
        [api = "smcVersion",             mapping = "computerDetails.smc_version"],
        [api = "nicSpeed",               mapping = "computerDetails.nic_speed"],
        [api = "opticalDrive",           mapping = "computerDetails.optical_drive"],
        [api = "bootRom",                mapping = "computerDetails.boot_rom"],
        [api = "bleCapable",             mapping = "computerDetails.ble_capable"]
