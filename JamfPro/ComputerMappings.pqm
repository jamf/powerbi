    UserAndLocationFields = {
        [api = "username",               mapping = "computerDetails.username"],
        [api = "realname",               mapping = "computerDetails.realname"],
        [api = "email",                  mapping = "computerDetails.email_address"],
        [api = "position",               mapping = "computerDetails.position"],
        [api = "phone",                  mapping = "computerDetails.phone"],
        [api = "departmentId",           mapping = "computerDetails.department"],
        [api = "buildingId",             mapping = "computerDetails.building"],
        [api = "room",                   mapping = "computerDetails.room"]
    },

    HardwareFields = {
        [api = "make",                   mapping = "computerDetails.make"],
        [api = "model",                  mapping = "computerDetails.model"],
        [api = "modelIdentifier",        mapping = "computerDetails.model_identifier"],
        [api = "serialNumber",           mapping = "computerDetails.serial_number"],
        [api = "processorSpeedMhz",      mapping = "computerDetails.processor_speed_mhz"],
