[
    ApiFields = (mappings as list)  as list => List.Transform(mappings, each _[api]),
    TableFields = (mappings as list ) as list => List.Transform(mappings, each _[mapped]),

    GeneralFields = {
        [api = "name",                     mapping = "name"],
        [api = "lastIpAddress",            mapping = "computerDetails.ip_address"],
        [api = "lastReportedIp",           mapping = "computerDetails.last_reported_ip"],
        [api = "jamfBinaryVersion",        mapping = "computerDetails.jamf_version"],
        [api = "platform",                 mapping = "computerDetails.platform"],
        [api = "barcode1",                 mapping = "computerDetails.barcode_1"],
        [api = "barcode2",                 mapping = "computerDetails.barcode_2"],
        [api = "assetTag",                 mapping = "computerDetails.asset_tag"],
        [api = "remoteManagement",         mapping = "remoteManagement"],
        [api = "mdmCapable",               mapping = "mdmCapable"],
        [api = "reportDate",               mapping = "computerDetails.report_date_utc"],
        [api = "lastContactTime",          mapping = "computerDetails.last_contact_time_utc"],
        [api = "lastCloudBackupDate",      mapping = "computerDetails.last_cloud_backup_date_utc"],
        [api = "lastEnrolledDate",         mapping = "computerDetails.last_enrolled_date_utc"],
        [api = "initialEntryDate",         mapping = "computerDetails.initial_entry_date_utc"],
        [api = "distributionPoint",        mapping = "computerDetails.distribution_point"],
        [api = "site",                     mapping = "computerDetails.site"],
        [api = "itunesStoreAccountActive", mapping = "computerDetails.itunes_store_account_is_active"]
        },

    RemoteManagementFields = {
        [api = "managed",                mapping = "computerDetails.remote_management.managed"],
        [api = "managementUsername",     mapping = "computerDetails.remote_management.management_username"]
    },

   MdmCapableFields = {
        [api = "capable",                mapping = "computerDetails.mdm_capable"],
        [api = "capableUsers",           mapping = "computerDetails.mdm_capable_users.mdm_capable_user"]
   },

   PurchasingFields = {
        [api = "purchased",              mapping = "computerDetails.is_purchased"],
        [api = "leased",                 mapping = "computerDetails.is_leased"],
        [api = "poNumber",               mapping = "computerDetails.po_number"],
        [api = "lifeExpectancy",         mapping = "computerDetails.life_expectancy"],
        [api = "purchasePrice",          mapping = "computerDetails.purchase_price"],
        [api = "purchasingAccount",      mapping = "computerDetails.purchasing_account"],
        [api = "purchasingContact",      mapping = "computerDetails.purchasing_contact"],
        [api = "appleCareId",            mapping = "computerDetails.applecare_id"],
        [api = "vendor",                 mapping = "computerDetails.vendor"],
